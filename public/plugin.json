{
  "pluginName": "tinypng压缩图片",
  "description": "使用tinypng压缩图片",
  "author": "csj8520",
  "homepage": "https://github.com/csj8520/utools-plugin-tinypng.git",
  "main": "index.html",
  "version": "1.1.2",
  "logo": "logo.png",
  "preload": "preload.js",
  "features": [
    {
      "code": "tinypng",
      "explain": "压缩图片",
      "cmds": [
        "tinypng",
        "压缩图片",
        {
          "type": "files",
          "fileType": "file",
          "match": "/\\.(png|jpeg|jpg)$/i",
          "label": "压缩图片"
        },
        {
          "type": "files",
          "fileType": "directory",
          "label": "压缩此文件夹下所有图片"
        },
        {
          "type": "window",
          "match": { "app": ["Finder.app", "explorer.exe", "SearchApp.exe"], "class": ["CabinetWClass", "ExploreWClass"] },
          "label": "压缩此文件夹下所有图片"
        }
      ]
    }
  ]
}
